@model HrbiApp.Web.Models.LabServices.CreateLabServiceModel

@{
    ViewData["Title"] = "Create";
}
<div class="tile mb-4">
    <div class="page-header">
        <div class="row">
            <div class="col-lg-12">
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ServiceNameAR" class="control-label"></label>
                <input asp-for="ServiceNameAR" class="form-control" />
                <span asp-validation-for="ServiceNameAR" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ServiceNameEN" class="control-label"></label>
                <input asp-for="ServiceNameEN" class="form-control" />
                <span asp-validation-for="ServiceNameEN" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Price" class="control-label"></label>
                <input asp-for="Price" class="form-control" />
                <span asp-validation-for="Price" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsAvilableFromHome" /> @Html.DisplayNameFor(model => model.IsAvilableFromHome)
                </label>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<div class="col-lg-12">
</div>
<div style="text-align:left">
    <a asp-action="Index">
        <button class="btn btn-outline-secondary" type="button">
            @HrbiApp.Web.Resource.ResourceAr.Back<i class="fa fa-share" aria-hidden="true"></i>
        </button>
    </a>
</div>
</div>>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
