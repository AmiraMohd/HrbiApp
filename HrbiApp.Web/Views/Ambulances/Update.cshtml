@model HrbiApp.Web.Models.Ambulances.UpdateAmbulanceModel

@{
    ViewData["Title"] = "Update";
}
<div class="tile mb-4">
    <div class="page-header">
        <div class="row">
            <div class="col-lg-12">
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
        <form asp-action="Update">
            <div asp-validation-summary="All" class="text-danger"></div>
            <input asp-for="ID" class="form-control" hidden />
            <div class="form-group">
                <label asp-for="Phone" class="control-label"></label>
                <input asp-for="Phone" class="form-control" />
                <span asp-validation-for="Phone" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Hospital" class="control-label"></label>
                <input asp-for="Hospital" class="form-control" />
                <span asp-validation-for="Hospital" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<div class="col-lg-12">
</div>
<div style="text-align:left">
    <a asp-action="Index">
        <button class="btn btn-outline-secondary" type="button">
            @HrbiApp.Web.Resource.ResourceAr.Back<i class="fa fa-share" aria-hidden="true"></i>
        </button>
    </a>
</div>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
